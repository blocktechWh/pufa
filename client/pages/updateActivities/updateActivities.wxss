/* pages/updateActivities/updateActivities.wxss */
/* pages/contact/contacts.wxss */
@import "../../weui.wxss";

.newCount_wrap {
  width: 100%;
  height: auto;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
}

.check_title {
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 30rpx;
  margin-top: 10rpx;
  background: #4e8cee;
  color: #fff;
  box-sizing: border-box;
  padding-left: 30rpx;
}

.check_person {
  width: 100%;
  height: auto;
  overflow: hidden;
}

.check_pesrons_box {
  width: 20%;
  float: left;
}

.check_pesrons_img {
  box-sizing: border-box;
  width: 90rpx;
  height: 90rpx;
  margin: 28rpx auto 0;
  display: block;
}

.checked_pesrons_img {
  box-sizing: border-box;
  width: 90rpx;
  height: 90rpx;
  border: 6rpx solid greenyellow;
  margin: 28rpx auto 0;
  display: block;
}

.check_pesrons_text {
  text-align: center;
  font-size: 20rpx;
  color: #000;
  line-height: 44rpx;
}
.create-sure{
  padding:34rpx;
}