@import "../../weui.wxss";
.results{
  width: 100%;
  height: auto;
}
.results_wrap{
  width: 100%;
  height: auto;
  display: flex;
  padding-top:20rpx;
  box-sizing: border-box; 
}
.results_box{
  flex: 1;
  height: 160rpx;

}
.results_box_img{
  display: block;
  width: 100rpx;
  height: 100rpx;
  margin: 10rpx auto 0;
  border-radius: 50%;
}
.results_box_name{
  text-align: center;
  font-size: 30rpx;
  color: #000;

}
.results_box_arrow{
  width: 100%;
  height: 50rpx;
  display: block;
}
