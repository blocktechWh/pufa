/* pages/answerAction/answerAction.wxss */
.page {
  position: fixed;
  z-index: 1;
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.bac-img {
  width: 750rpx;
  height: 100%;
}
.question-container{
  position: absolute;
  width: 690rpx;
  height: 1090rpx;
  background-color: rgb(254, 147, 90);

  border-radius: 12rpx;
  top: 20rpx;
  left: 30rpx;
}
.question-container .question{
  height: 392rpx;
  padding-top: 160rpx;
  text-align: center;
  color: #fff;
}
.question-container .selects>view{
  width: 620rpx;
  height: 90rpx;
  line-height: 90rpx;
  color: rgb(96, 50, 35);
  font-size: 28rpx;
  text-align: center;
  border-radius: 10rpx;
  background-color: rgb(255, 206, 180);
  margin: 20rpx auto;
}