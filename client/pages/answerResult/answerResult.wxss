/* pages/answerResult/answerResult.wxss */
.page {
  position: fixed;
  z-index: 1;
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.bac-img {
  width: 750rpx;
  height: 100%;
}
.result-container{
  position: absolute;
  width: 690rpx;
  height: 1090rpx;
  background-color: rgb(254, 147, 90);
  border-radius: 12rpx;
  top: 20rpx;
  left: 30rpx;
}
.result-container>view:first-of-type{
  margin-top: 292rpx;
  font-size: 36rpx;
  color: rgb(96, 50, 35);
}
.result-container>view:nth-child(2){
  margin-top: 40rpx;
  font-size: 48rpx;
  color: #fff;
}
.result-container>view:nth-child(3){
  margin-top: 30rpx;
  font-size: 36rpx;
  color: rgb(96, 50, 35);
}
.result-container>view:nth-child(4){
  margin-top: 30rpx;
  font-size: 36rpx;
  color: rgb(96, 50, 35);
}
.result-container>button{
  width: 600rpx;
  height: 86rpx;
  background-color: rgb(255, 223, 181);
  line-height: 86rpx;
  font-size: 32rpx;
  color: rgb(96, 50, 35);
  margin-top: 302rpx;
}