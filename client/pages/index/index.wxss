.page{
  position: fixed;
  width: 100%;
  height: 100%;
}
.index-header{
  height: 238rpx;
  background-color: yellow;
  position: relative;
}
.index-header image:first-of-type{
  position: absolute;
  z-index: 1;
  width: 100%;
  height: 100%;
}
.userinfo-avatar{
  position: absolute;
  z-index: 2;
  width: 120rpx;
  height: 120rpx;
  border-radius: 60rpx;
  margin-top: 52rpx;
  margin-left: 30rpx;
  float: left
}
.userinfo-nickname{
  z-index: 2;
  position: absolute;
  left: 150rpx;
  margin-left: 60rpx;
  margin-top: 52rpx;
  display: inline-block;
  height: 120rpx;
  float: left;
  font-size: 32rpx;
  color: #fff;
  line-height: 120rpx

}
.my-integral{
  z-index: 2;
  position: absolute;
  right: 30rpx;
  margin-top: 52rpx;
  display: inline-block;
  height: 120rpx;
  float: right;
  font-size: 32rpx;
  color: #fff;
  line-height: 120rpx
}
.index-content{
  background-color: #fff;
}
.index-content>view>navigator{
  border: 1rpx solid #ededed;
}
.share-luck-draw{
  background: none;
  height: 100%;
}
.index-content-one,.index-content-two,.index-content-three{
  float: left;
}
.index-content-one,.index-content-three{
  width: 200rpx;
}
.index-content-two{
  width: 350rpx;

}
.index-content-one>view,.index-content-three>view{
  height: 320rpx;
}
.index-content-two>navigator:first-of-type{
  height: 200rpx
}
.index-content-two>navigator:nth-child(2){
  height: 240rpx
}
.index-content-two>navigator:nth-child(3){
  height: 200rpx
}
.menu-box{
  text-align: center;
}
.menu-box-one image{
  width: 106rpx;
  height: 130rpx;
  display: block;
  margin: 0 auto
}
.menu-box-two image{
  width: 120rpx;
  height: 127rpx;
  display: block;
  margin: 0 auto
}
.menu-box-three image{
  width: 140rpx;
  height: 182rpx;
  display: block;
  margin: 0 auto
}

.index-footer{
  position: relative;
  height: 285rpx;
  width: 750rpx;
  background-color: #ccc;
}
.index-footer image:nth-child(1){
  position: absolute;
  height: 55rpx;
  width: 180rpx;
  top: 190rpx;
  left: 140rpx;
}
.index-footer image:nth-child(2){
  width: 750rpx;
  height: 285rpx;
}
