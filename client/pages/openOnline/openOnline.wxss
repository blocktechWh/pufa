/* pages/openOnline/openOnline.wxss */
.credit-card-show image{
  width: 100%;
  height: 416rpx;
}
.next-step{
  background-color: rgb(89, 123, 255);
  height: 70rpx;
  color: rgb(254, 254, 254);
  font-size: 26rpx;
  line-height: 70rpx;
}
.credit-card-describ{
  background-color: #fff;
  padding: 0 35rpx;

}
.credit-card-describ .credit-card-describ-title{
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #000000;
  font-size: 32rpx;
}
.credit-card-describ .credit-card-describ-text{
  position: relative;
  height: 56rpx;
  line-height: 56rpx;
  font-size: 28rpx;
  padding-left: 32rpx;
}
.credit-card-describ .credit-card-describ-text image{
  position: absolute;
  top: 12rpx;
  left: 0;
  width: 32rpx;
  height: 32rpx;
}